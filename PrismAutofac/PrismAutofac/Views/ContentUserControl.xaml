<UserControl x:Class="PrismAutofac.Views.ContentUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <Button Content="Button1" 
                    Command="{Binding NavigateCommand}" CommandParameter="PropertyGridUserControl"
                    HorizontalAlignment="Left" Margin="20,10,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Button2" 
                    Command="{Binding Button2Command}"
                    HorizontalAlignment="Left" Margin="20,35,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</UserControl>
